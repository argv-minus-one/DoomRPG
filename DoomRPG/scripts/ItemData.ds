            if (i == 1 || i == 7)
                continue;
            
            for (int j = 0; j < ItemMax[i]; j++)
            {
                // Don't pick cheap stuff like a Pistol or Armor Bonus
                if (ItemData[i][j].Price <= 100)
                    continue;
                
                RewardList[MaxRewards] = &ItemData[i][j];
                
